<resources>
    <declare-styleable name="StoreEvaluation">
        <attr name="re_style_new" format="reference" />
        <attr name="re_style_new_star_size" format="dimension" />
        <attr name="re_style_new_text_size" format="dimension" />
        <attr name="re_style_quantity" format="reference" />
        <attr name="re_style_quantity_text_size" format="dimension" />
        <attr name="re_style_rate" format="reference" />
        <attr name="re_style_rate_text_size" format="dimension" />
        <attr name="lb_background" format="reference"></attr>
        <attr name="lb_textColor" format="reference"></attr>
    </declare-styleable>

    <!-- Declare custom theme attributes that allow changing which styles are
         used for button bars depending on the API level.
         ?android:attr/buttonBarStyle is new as of API 11 so this is
         necessary to support previous API levels. -->
    <declare-styleable name="ButtonBarContainerTheme">
        <attr name="metaButtonBarStyle" format="reference" />
        <attr name="metaButtonBarButtonStyle" format="reference" />
    </declare-styleable>
    <declare-styleable name="FullscreenAttrs">
        <attr name="fullscreenBackgroundColor" format="color" />
        <attr name="fullscreenTextColor" format="color" />
    </declare-styleable>

    <declare-styleable name="CustomRatingBar">
    <attr name="crb_emptyIcon" format="reference">
    </attr>
    <attr name="crb_emptyIconTint" format="reference">
    </attr>
    <attr name="crb_filledHalfIcon" format="reference">
    </attr>
    <attr name="crb_filledIcon" format="reference">
    </attr>
    <attr name="crb_filledIconTint" format="reference">
    </attr>
    <attr name="crb_iconHeight" format="dimension">
    </attr>
    <attr name="crb_margin" format="dimension">
    </attr>
    <attr name="crb_maxRating" format="integer">
    </attr>
    <attr name="crb_rating" format="float">
    </attr>
    </declare-styleable>

    <attr name="ds_lb_loadingColor" format="color">
    </attr>

    <attr name="ds_lb_drawableLeft" format="reference">
    </attr>
</resources>
